 <!-- CSR Ranking  -->
 <div class="container-xxl flex-grow-1 container-p-y">
  <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 gx-4"> 

    <% csrRanking.forEach(function(csr, idx) { %>
      <div class="col-xl-4 mb-4 col-lg-5 col-12">
        <div class="card">
          <div class="d-flex align-items-end row">
            <div class="col-7">
              <div class="card-body text-nowrap">
                <h5 class="card-title mb-0">
                  Congratulations <%= csr.csr %>! ðŸŽ‰
                  
                </h5>
                <p class="mb-0"><%= csr.location %></p>
                <p class="mb-2">
                  <%= idx === 0 ? '1st ' : idx === 1 ? '2nd' : '3rd' %> Place P&amp;C Sales Last Week
                </p>
                <h4 class="text-primary mb-1"><%= csr.premium %> / <%= csr.policies %></h4>
                <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#csrModal<%= idx %>">
                  By Carriers
                </button>
              </div>
            </div>
            <div class="col-5 text-center text-sm-left">
