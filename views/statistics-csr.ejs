}

#csr-ranking-table td a.csr-detail-link:hover,
#csr-ranking-table td a.csr-detail-link:focus {
  color: #f5a706 !important;
  text-shadow: 0 2px 14px #f5a70666, 0 0 6px #f5a70644;
