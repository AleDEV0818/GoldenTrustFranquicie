<style>

/* Loader SweetAlert2 glass effect */
.swal2-glass-effect {
  border-radius: 1.5rem !important;
  box-shadow: 0 16px 48px 0 rgba(31, 38, 135, 0.37) !important;
  background: rgba(30,34,54,0.92) !important;
  backdrop-filter: blur(6px);
  border: 1px solid rgba(255,255,255,0.08) !important;
}

/* Calendario (daterangepicker) glass style */
.daterangepicker {
  border-radius: 1rem !important;
  box-shadow: 0 8px 32px 0 rgba(31, 38, 135, 0.37) !important;
  background: rgba(30,34,54,0.92) !important;
  color: #fff !important;
  backdrop-filter: blur(8px);
  border: 1px solid rgba(255,255,255,0.18) !important;
}
.daterangepicker .calendar-table {
  border-radius: .75rem !important;
  background: rgba(40,44,74,0.93) !important;
  color: #fff !important;
}
.daterangepicker td.available, .daterangepicker th {
  color: #fff !important;
}

/* Cambia el color del rango seleccionado a VERDE claro y números oscuros */
.daterangepicker td.in-range {
  background: #81C784 !important;      /* Verde claro */
  color: #222 !important;              /* Números oscuros y visibles */
}

/* Inicio y fin del rango, más destacados aún */
.daterangepicker td.start-date,
.daterangepicker td.end-date {
  background: #388E3C !important;      /* Verde fuerte */
  color: #fff !important;
  font-weight: bold;
  border-radius: 0.5em !important;
}

/* Día activo (cuando haces click) */
.daterangepicker td.active, .daterangepicker td.active:hover {
  background: #388E3C !important;
  color: #fff !important;
}

/* Rango rápido seleccionado */
.daterangepicker .ranges li.active {
  background: #388E3C !important;
  color: #fff !important;
}

/* Mejorar contraste hover sobre días disponibles */
.daterangepicker td.available:hover {
  background: #81C784cc !important; /* Verde claro con opacidad */
  color: #111 !important;
}

/* Mantener bordes suaves y fondo glassy */
.daterangepicker .calendar-table {
  box-shadow: 0 2px 12px 0 rgba(31, 38, 135, 0.15) !important;
}



/* TOP 3 - */

.datatables-csr-ranking tbody tr.gold-row {
  background: linear-gradient(90deg,
    rgba(255, 240, 187, 0.30) 0%,
    rgba(242, 215, 91, 0.22) 40%,
    rgba(229, 178, 51, 0.18) 100%
  );
  border-left: 4px solid #FFD700;
  font-size: 1.12em;
  font-weight: 600;
  color: #735C13;
}
.datatables-csr-ranking tbody tr.silver-row {
  background: linear-gradient(90deg,
    rgba(240, 242, 245, 0.33) 0%,
    rgba(200, 206, 219, 0.22) 40%,
    rgba(150, 157, 169, 0.16) 100%
  );
  border-left: 4px solid #B0C4DE;
  font-size: 1.08em;
  font-weight: 600;
  color: #232323; /* Gris muy oscuro para fondo claro */
}
.datatables-csr-ranking tbody tr.bronze-row {
  background: linear-gradient(90deg,
    rgba(231, 173, 104, 0.19) 0%,
    rgba(205, 135, 60, 0.17) 40%,
    rgba(139, 93, 60, 0.13) 100%
  );
  border-left: 4px solid #B87333;
  font-size: 1.06em;
  font-weight: 600;
  color: #6C4321;
}

/* Consistencia en celdas */
.datatables-csr-ranking tbody tr.gold-row td,
.datatables-csr-ranking tbody tr.silver-row td,
.datatables-csr-ranking tbody tr.bronze-row td {
  background-color: inherit !important;
  color: inherit !important;
}

/* Hover sutil y elegante */
.datatables-csr-ranking tbody tr.gold-row:hover,
.datatables-csr-ranking tbody tr.silver-row:hover,
.datatables-csr-ranking tbody tr.bronze-row:hover {
  filter: brightness(1.08) saturate(1.13);
  box-shadow: 0 2px 12px 0 rgba(80,80,0,0.11);
}

/* Mejor contraste para modo oscuro */
@media (prefers-color-scheme: dark) {
  .datatables-csr-ranking tbody tr.gold-row {
    color: #FFE066;
    border-left-color: #FFD700;
  }
  .datatables-csr-ranking tbody tr.silver-row {
    color: #D3D9DE;
    border-left-color: #B0C4DE;
  }
  .datatables-csr-ranking tbody tr.bronze-row {
    color: #FFD2A6;
    border-left-color: #B87333;
  }
}

/* Export  */
div.dt-button-collection {
  background: var(--bs-card-bg, #23243a) !important;
  color: var(--bs-body-color, #fff) !important;
  border-radius: 0.5rem !important;
  box-shadow: 0 4px 24px rgba(0,0,0,0.18);
}
div.dt-button-collection .dt-button {
  color: var(--bs-body-color, #fff) !important;
}
div.dt-button-collection .dt-button:hover {
  background: var(--bs-primary, #5370f0) !important;
  color: #fff !important;
}
/* Export */
.dt-export-buttons {
  min-width: 160px;
  margin-right: 14px;
}
.dt-export-buttons .btn {
  margin-left: 12px;
  box-shadow: 0 2px 8px rgba(80,80,80,0.08);
