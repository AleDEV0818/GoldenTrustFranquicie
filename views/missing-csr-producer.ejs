<%
// Build a query string for nav links that preserves location and active state
const navParams = [];
if (typeof locationId !== 'undefined' && locationId) {
  navParams.push('location=' + encodeURIComponent(locationId));
}
if (typeof activeOnly !== 'undefined' && activeOnly) {
  navParams.push('active=1');
}
const navQS = navParams.length ? ('?' + navParams.join('&')) : '';
%>

<ul class="nav nav-pills flex-column flex-md-row mb-4 mx-2 mx-md-4">

  <li class="nav-item">
    <a
      class="nav-link <%= (currentPath || '').startsWith('/users/policy-report-by-location') ? 'active' : '' %>"
      href="/users/policy-report-by-location<%= navQS %>"
    >
      <i class="ti ti-report ti-xs me-1"></i>
      Binder Errors
    </a>
  </li>

  <li class="nav-item">
    <a
      class="nav-link <%= (currentPath || '').startsWith('/users/missing-csr-producer') ? 'active' : '' %>"
      href="/users/missing-csr-producer<%= navQS %>"
    >
      <i class="ti ti-user-exclamation ti-xs me-1"></i>
      Missing CSR/Producer
    </a>
  </li>

  <li class="nav-item">
    <a
      class="nav-link <%= (currentPath || '').startsWith('/users/missing-contact-errors') ? 'active' : '' %>"
      href="/users/missing-contact-errors<%= navQS %>"
    >
      <i class="ti ti-mail-x ti-xs me-1"></i>
      Missing Contact Info
    </a>
  </li>

</ul>

<div class="mx-2 mx-md-4 mb-4">
  <div class="filter-toolbar shadow-sm">
    <div class="d-flex flex-wrap align-items-stretch gap-3">

      <!-- Grupo: Fechas -->
      <div class="ft-group">
        <div class="ft-header">
          <span class="ft-icon" aria-hidden="true"><i class="ti ti-calendar"></i></span>
          <span class="ft-label">Dates</span>
        </div>
        <div class="ft-body">
          <div class="input-group input-group-sm">
            <input
              type="text"
              id="mcp-start"
              class="form-control glass-calendar"
              value="<%= initialRange?.startISO || '' %>"
              placeholder="YYYY-MM-DD"
              aria-label="Start date"
              autocomplete="off"
            />
          </div>
          <span class="ft-sep">to</span>
          <div class="input-group input-group-sm">
            <input
              type="text"
              id="mcp-end"
              class="form-control glass-calendar"
              value="<%= initialRange?.endISO || '' %>"
              placeholder="YYYY-MM-DD"
              aria-label="End date"
              autocomplete="off"
            />
          </div>
        </div>
      </div>

      <div class="ft-vr" aria-hidden="true"></div>

      <!-- Grupo: Franquicia (Corporate) -->
      <% if (user.location_type === 1 && Array.isArray(franchises) && franchises.length > 0) { %>
        <% var selectedId = typeof locationId !== 'undefined' ? locationId : ""; %>
        <div class="ft-group">
          <div class="ft-header">
            <span class="ft-icon" aria-hidden="true"><i class="ti ti-building-store"></i></span>
            <span class="ft-label">Franchise</span>
          </div>
          <div class="ft-body">
            <div class="input-group input-group-sm" style="min-width: 220px;">
              <span class="input-group-text"><i class="ti ti-map-pin"></i></span>
              <select id="location-select" class="form-select">
                <% franchises.forEach(function(franchise) { %>
                  <option value="<%= franchise.location_id %>" <%= String(franchise.location_id) === String(selectedId) ? 'selected' : '' %>>
                    <%= franchise.alias %>
                  </option>
                <% }); %>
              </select>
            </div>
          </div>
        </div>
      <% } else { %>
        <!-- Grupo: Franchise (fijo, no corporativo) -->
        <div class="ft-group">
          <div class="ft-header">
            <span class="ft-icon" aria-hidden="true"><i class="ti ti-building"></i></span>
            <span class="ft-label">Franchise</span>
          </div>
          <div class="ft-body">
            <span class="badge bg-secondary-subtle text-secondary-emphasis">
              <%= user.location_alias || user.alias || user.location_name || "‚Äî" %>
            </span>
          </div>
        </div>
      <% } %>

      <div class="ft-vr" aria-hidden="true"></div>

      <!-- Grupo: Active -->
      <div class="ft-group">
        <div class="ft-header">
          <span class="ft-icon" aria-hidden="true"><i class="ti ti-activity"></i></span>
          <span class="ft-label">Active</span>
        </div>
        <div class="ft-body d-flex align-items-center">
          <div class="form-check form-switch m-0">
            <input
              class="form-check-input"
              type="checkbox"
              role="switch"
              id="mcp-active-only"
              value="1"
              <%= activeOnly ? 'checked' : '' %>
              aria-label="Only active policies"
            />
            <label class="form-check-label ms-1" for="mcp-active-only">Only</label>
          </div>
        </div>
      </div>

      <!-- Apply -->
      <div class="ft-group ft-apply">
        <div class="ft-body w-100">
          <button class="btn btn-primary btn-sm w-100" id="mcp-apply" type="button">Apply</button>
        </div>
      </div>

    </div>
  </div>
</div>

<!-- KPIs debajo de filtros -->
<div class="mx-2 mx-md-4 mt-2">
  <div class="kpi-bars" role="region" aria-label="Summary KPIs">
    <!-- Active Policies -->
    <div class="kpi-bar kpi--blue" role="status" aria-live="polite">
      <div class="kpi-icon" aria-hidden="true"><i class="ti ti-sum"></i></div>
      <div class="kpi-content">
        <div class="kpi-label">Active Policies</div>
        <div class="kpi-value" id="mcp-active-policies-count">
          <%= typeof activePoliciesCount === 'number' ? activePoliciesCount.toLocaleString('en-US') : '0' %>
        </div>
      </div>
    </div>

    <!-- Missing CSR/Producer => total visible en DataTable -->
    <div class="kpi-bar kpi--red" role="status" aria-live="polite">
      <div class="kpi-icon" aria-hidden="true"><i class="ti ti-alert-triangle"></i></div>
      <div class="kpi-content">
        <div class="kpi-label">Missing CSR/Producer</div>
        <div class="kpi-value" id="mcp-missing-total-count">0</div>
      </div>
    </div>
  </div>
</div>

<!-- Tabla -->
<div class="mx-2 mx-md-4 my-4">
  <div class="card shadow">
    <div class="card-body">
      <div class="table-responsive">
        <table
          class="datatable-missing-csr-producer table table-striped table-bordered align-middle mb-0"
          style="width:100%;"
        >
          <thead>
            <tr>
              <th>Policy #</th>
              <th>Line of Business</th>
              <th>Business Type</th>
              <th>CSR</th>
              <th>Producer</th>
              <th>Binder Date</th>
              <th>Effective Date</th>
              <th>Location</th>
              <th>Policy Status</th>
            </tr>
          </thead>
          <tbody></tbody>
        </table>
      </div>
      <!-- El bot√≥n Export CSV lo inserta DataTables (dom: 'B...') -->
    </div>
  </div>
</div>

