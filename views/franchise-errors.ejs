<!-- Franchise Errors â Filtros (mismo estilo que MCP: toolbar + ft-group + Ã­conos) -->
<div class="mx-2 mx-md-4 mb-4">
  <div class="filter-toolbar shadow-sm">
    <div class="d-flex flex-wrap align-items-stretch gap-3">

      <!-- Grupo: Fechas -->
      <div class="ft-group">
        <div class="ft-header">
          <span class="ft-icon" aria-hidden="true"><i class="ti ti-calendar"></i></span>
          <span class="ft-label">Dates</span>
        </div>
        <div class="ft-body">
          <div class="input-group input-group-sm">
            <input
              type="text"
              id="fr-errors-start"
              class="form-control glass-calendar"
              value="<%= initialRange?.startISO || '' %>"
              placeholder="YYYY-MM-DD"
              aria-label="Start date"
              autocomplete="off"
            />
          </div>
          <span class="ft-sep">to</span>
          <div class="input-group input-group-sm">
            <input
              type="text"
              id="fr-errors-end"
              class="form-control glass-calendar"
              value="<%= initialRange?.endISO || '' %>"
              placeholder="YYYY-MM-DD"
              aria-label="End date"
              autocomplete="off"
            />
          </div>
        </div>
      </div>

      <div class="ft-vr" aria-hidden="true"></div>

      <!-- Grupo: Active -->
      <div class="ft-group">
        <div class="ft-header">
          <span class="ft-icon" aria-hidden="true"><i class="ti ti-activity"></i></span>
          <span class="ft-label">Active</span>
        </div>
        <div class="ft-body d-flex align-items-center">
          <div class="form-check form-switch m-0">
            <input
              class="form-check-input"
              type="checkbox"
              role="switch"
              id="frx-active-only"
              value="1"
              <%= activeOnly ? 'checked' : '' %>
              aria-label="Only active"
            />
            <label class="form-check-label ms-1" for="frx-active-only">Only</label>
          </div>
        </div>
      </div>

      <!-- Apply -->
      <div class="ft-group ft-apply">
        <div class="ft-body w-100">
          <button class="btn btn-primary btn-sm w-100" id="fr-errors-apply" type="button">Apply</button>
        </div>
      </div>



    </div>
  </div>
</div>

