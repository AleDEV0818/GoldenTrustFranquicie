<!-- MAIN CONTENT CODE TABLE -->
<div class="container-xxl flex-grow-1 container-p-y">
  <h4 class="fw-bold py-3 mb-4">Access Codes</h4>
  <div class="card mb-4 shadow-sm border-0">
    <div class="card-body">
      <form id="codes-filters-form">
        <div class="row gy-3 gx-4 align-items-end">
          <% if (user.location_type === 1) { %>
            <!-- CORPORATE: Agency select filter -->
            <div class="col-12 col-md-6 col-lg-3" id="agency-selector-col">
              <label for="filter-agency" class="filter-label">
                <i class="bi bi-building text-primary"></i> Agency
              </label>
              <select id="filter-agency"
                      class="selectpicker w-100"
                      data-live-search="true"
                      title="Select Agency"
                      name="agency">
                <!-- Options rendered by JS -->
              </select>
            </div>
          <% } else if (user.location_type === 2 || user.location_type === 4) { %>
            <!-- FRANCHISE: Show alias only (not a select) -->
            <div class="col-12 col-md-6 col-lg-3" id="agency-alias-display">
              <label class="filter-label">
                <i class="bi bi-building text-primary"></i> Agency
              </label>
              <div class="form-control-plaintext fw-bold ps-2">
                <%= user.agency_alias %>
                <input type="hidden" id="filter-agency" name="agency" value="<%= user.agency_alias %>">
              </div>
            </div>
          <% } %>

          <div class="col-12 col-md-6 col-lg-3">
            <label for="filter-company" class="filter-label">
              <i class="bi bi-briefcase text-info"></i> Company
            </label>
            <select id="filter-company"
                    class="selectpicker w-100"
                    data-live-search="true"
                    title="Select Company"
                    name="company">
              <!-- Options rendered by JS -->
            </select>
          </div>
          <div class="col-12 col-md-6 col-lg-6 d-flex flex-column flex-sm-row gap-2 justify-content-end align-items-sm-center">
            <button type="button" class="btn btn-outline-secondary" id="reset-filters-btn">
              <i class="bi bi-arrow-clockwise"></i> Reset Filters
            </button>
            <button type="button" class="btn btn-outline-primary fw-semibold px-4" id="export-btn">
              <i class="bi bi-download"></i> Export CSV
            </button>
            <input type="file" class="d-none" id="excel-input" name="file" accept=".xlsx,.xls,.csv">
            <button type="button" class="btn btn-outline-success fw-semibold px-4" id="import-btn">
              <i class="bi bi-upload"></i> Import
            </button>
          </div>
        </div>
      </form>
      <div id="import-result" class="mb-3 fs-6"></div>
      <div class="card mt-4 shadow-sm border-0">
        <div class="card-datatable table-responsive">
          <table class="table table-bordered table-hover align-middle shadow-sm rounded" id="codes-table"
                 aria-describedby="codes-table-description">
            <caption id="codes-table-description" class="visually-hidden">
              Table of agency/company codes, logins and passwords.
            </caption>
            <thead class="table-light">
              <tr>
                <th class="text-nowrap" scope="col">AGENCY</th>
                <th class="text-nowrap" scope="col">COMPANY</th>
                <th class="text-nowrap" scope="col">CODE</th>
                <th class="text-nowrap" scope="col">LOGIN</th>
                <th class="text-nowrap" scope="col">PASSWORD</th>
              </tr>
            </thead>
            <tbody>
              <!-- Data rendered by JS -->
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- END MAIN CONTENT -->


      </div>
    </div>
  </div>
